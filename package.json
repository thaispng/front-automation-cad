{
  "name": "front-automation-cad",
  "version": "0.1.0",
  "description": "Front Automation CAD",
  "author": "Thais <thaisvtr26@gmail.com>",
  "main": "main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "next build && electron-builder"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "lucide-react": "^0.383.0",
    "next": "^14.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^7.6.0",
    "electron": "^31.0.2",
    "electron-builder": "^22.14.13",
    "electron-is-dev": "^3.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "wait-on": "^6.0.1"
  },
  "build": {
    "appId": "AutomationCAD",
    "files": [
      "main.js",
      "preload.js",
      "out",
      "node_modules",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
